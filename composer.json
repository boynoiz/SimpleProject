{
    "name": "boynoiz/simpleFramework",
    "description": "Simple Framework for starter kit",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "ext-pdo": "*",
        "symfony/class-loader": "^3.1",
        "symfony/var-dumper": "^3.1",
        "zendframework/zend-config": "^2.6",
        "symfony/filesystem": "^3.1",
    },
    "require-dev": {
        "maximebf/debugbar": "1.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "minimum-stability": "dev"
    }
}
